  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:setup": "bash scripts/setup-databases.sh",
    "db:setup:win": "powershell -ExecutionPolicy Bypass -File scripts/setup-databases.ps1",
    "test:e2e:verbose": "jest --config ./test/jest-e2e.json --verbose --no-silent",
    "test:acceptance": "jest --config ./test/jest-e2e.json test/acceptance --verbose --no-silent",
    "test:kafka": "jest --config ./test/jest-e2e.json test/kyc/simple-kafka-webhook.e2e-spec.ts --verbose --no-silent --detectOpenHandles --forceExit",
    "test:kafka-full": "jest --config ./test/jest-kafka-integration.json --verbose --no-silent --detectOpenHandles --forceExit",
    "test:kafka-simplified": "jest --config ./test/jest-e2e.json test/kyc/kafka-webhook-simplified.e2e-spec.ts --verbose --no-silent --detectOpenHandles --forceExit",
    "test:kafka-mock": "jest --config ./test/jest-e2e.json test/kyc/kafka-webhook-mock.e2e-spec.ts --verbose --no-silent --forceExit",
    "test:webhook-messagepattern": "jest --config ./test/kyc/jest-webhook-messagepattern.config.js --verbose --no-silent --forceExit",
    "test:webhook-messagepattern:direct": "ts-node test/kyc/run-webhook-messagepattern-test.ts",
    "test:webhook-isolated": "jest test/kyc/isolated-webhook-messagepattern.spec.ts --verbose",
    "test:webhook-database": "jest test/kyc/simple-database-webhook-messagepattern.spec.ts --verbose --forceExit",
    "test:webhook-integration": "jest test/kyc/final-integration-webhook.spec.ts --verbose --forceExit --detectOpenHandles",
    "test:acceptance:watch": "jest --config ./test/jest-e2e.json test/acceptance --verbose --no-silent --watch",
    "migration:run": "ts-node src/scripts/run-migration.ts",
    "migration:create": "typeorm mi gration:create",
    "db:seed": "ts-node src/scripts/seed-database.ts",
    "db:create": "ts-node src/scripts/creatteste-database.ts",
    "db:create-table": "npm run db:create && ts-node src/scripts/run-migration.ts"
